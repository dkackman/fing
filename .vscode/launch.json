    "version": "0.2.0",
    "configurations": [
        {
            "name": "Server",
            "type": "python",
            "request": "launch",
            "module": "generator.server",
            "cwd": "${workspaceFolder}/src/",
            "justMyCode": true
        },
        {
            "name": "CLI - init",
            "type": "python",
            "request": "launch",
            "module": "generator.main",
            "cwd": "${workspaceFolder}/src/",
            "args": [
                "init",
            ],
            "justMyCode": true
        },
        {
            "name": "CLI - txt2img",
            "type": "python",
            "request": "launch",
            "module": "generator.main",
            "cwd": "${workspaceFolder}/src/",
            "args": [
                "--prompt",
                "A gentleman otter in a 17th century portrait",
                "--outdir",
                "/mnt/ml/outputs",
            ],  
            "justMyCode": true
        },        
        {
            "name": "CLI - img2img",
            "type": "python",
            "request": "launch",
            "module": "generator.main",
            "cwd": "${workspaceFolder}/src/",
            "args": [
                "--prompt",
                "A landscape on mars",
                "--image_uri",
                "https://raw.githubusercontent.com/CompVis/stable-diffusion/main/assets/stable-samples/img2img/sketch-mountains-input.jpg",
                "--strength",
                "0.875",
                "--outdir",
                "/mnt/ml/outputs",
            ],
            "justMyCode": true
        },
        {
            "name": "CLI - imginpaint",
            "type": "python",
            "request": "launch",
            "module": "generator.main",
            "cwd": "${workspaceFolder}/src/",
            "args": [
                "--prompt",
                "A landscape on mars",
                "--image_uri",
                "https://raw.githubusercontent.com/CompVis/latent-diffusion/main/data/inpainting_examples/overture-creations-5sI6fQgYIuo.png",
                "--mask_uri",
                "https://raw.githubusercontent.com/CompVis/latent-diffusion/main/data/inpainting_examples/overture-creations-5sI6fQgYIuo_mask.png",
                "--strength",
                "0.875",
                "--outdir",
                "/mnt/ml/outputs",
            ],
            "justMyCode": true
        }
    ]
}