    "version": "0.2.0",
    "configurations": [
        {
            "name": "Gunicorn",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/generator",
            "program": "/mnt/ml/anaconda3/envs/fing/bin/gunicorn",
            "args": [
                "--bind",
                "0.0.0.0:9147",
                "server:gunicorn_app",
                "--timeout",
                "120"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Server",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/generator",
            "program": "server.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "CLI - txt2img",
            "type": "python",
            "request": "launch",
            "program": "cli.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/generator",
            "args": [
                "--prompt",
                "A gentleman otter in a 17th century portrait",
                "--num_images",
                "1"
            ],
            "justMyCode": true
        },
        {
            "name": "CLI - img2img",
            "type": "python",
            "request": "launch",
            "program": "cli.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/generator",
            "args": [
                "--prompt",
                "A landscape on mars",
                "--image_uri",
                "https://raw.githubusercontent.com/CompVis/stable-diffusion/main/assets/stable-samples/img2img/sketch-mountains-input.jpg",
                "--strength",
                "0.875"
            ],
            "justMyCode": true
        },
        {
            "name": "CLI - imginpaint",
            "type": "python",
            "request": "launch",
            "program": "cli.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/generator",
            "args": [
                "--prompt",
                "A landscape on mars",
                "--image_uri",
                "https://raw.githubusercontent.com/CompVis/latent-diffusion/main/data/inpainting_examples/overture-creations-5sI6fQgYIuo.png",
                "--mask_uri",
                "https://raw.githubusercontent.com/CompVis/latent-diffusion/main/data/inpainting_examples/overture-creations-5sI6fQgYIuo_mask.png",
                "--strength",
                "0.875"
            ],
            "justMyCode": true
        }
    ]
}